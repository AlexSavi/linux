/dts-v1/;
/* ConnectCore 6 (Quad wireless/bluetooth variant) */
#include "imx6q-ccimx6-wb.dtsi"
/* ConnectCore 6 SBC carrier board */
#include "imx6q-ccimx6sbc.dtsi"

&cpu0 {
	operating-points = <
		/* kHz    uV */
		1200000 1350000
		996000  1300000
		852000  1250000
		792000  1200000
		396000  1100000
		>;
	fsl,soc-operating-points = <
		/* ARM kHz  SOC-PU uV */
		1200000       1250000
		996000        1250000
		852000        1250000
		792000        1250000
		396000        1250000
		>;
	arm-supply = <&bcore1>;
	soc-supply = <&bcore2>;
};

&gpc {
	fsl,ldo-bypass = <1>; /* use ldo-bypass, u-boot will check it and configure */
};

&i2c2 {
	dialog@58 {
		regulators {
			bcore1 {
				regulator-min-microvolt = <3000>;
				regulator-max-microvolt = <1570000>;
				regulator-suspend-mem-microvolt = <1175000>;
				regulator-suspend-standby-microvolt = <1175000>;
			};

			bcore2 {
				regulator-min-microvolt = <3000>;
				regulator-max-microvolt = <1570000>;
				regulator-suspend-mem-microvolt = <1175000>;
				regulator-suspend-standby-microvolt = <1175000>;
			};
		};
	};
};

&hdmi_core {
	status = "okay";
};
